<%= form_for(upload) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>

    <%= f.fields_for :photos do |p| %>
        <%= p.label :image, 'Изображения' %>
        <%= p.file_field :image, multiple: true, name: 'upload[photos_attributes][image][]'%>
    <% end %>

    <%= f.submit button_title, class: 'btn btn-primary', id: 'submit_add_upload' %>
<% end %>